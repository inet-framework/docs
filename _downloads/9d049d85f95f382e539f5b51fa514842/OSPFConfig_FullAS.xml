<?xml version="1.0"?>
<OSPFASConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="OSPF.xsd">

  <!-- Routers -->
  <Router name="RA1" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RA2" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RA3" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RA4" RFC1583Compatible="true" >
    <BroadcastInterface ifName="eth0" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth3" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth4" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth2" />
    <BroadcastInterface ifName="eth1" />
  </Router>  
  
  <Router name="RB1" RFC1583Compatible="true" >
    <BroadcastInterface ifName="eth2" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth3" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth4" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RB2" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RB3" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RB4" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>  
  
  <Router name="RC1" RFC1583Compatible="true" >
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RC2" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth3" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RC3" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RC4" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router> 
  
  <Router name="RD1" RFC1583Compatible="true" >
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
  </Router>
  
  <Router name="RD2" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RD3" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth3" interfaceMode="NoOSPF" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router>
  
  <Router name="RD4" RFC1583Compatible="true">
    <BroadcastInterface ifName="eth2" interfaceMode="Passive" />
    <BroadcastInterface ifName="eth0" />
    <BroadcastInterface ifName="eth1" />
  </Router> 

</OSPFASConfig>
